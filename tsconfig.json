{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "typeRoots": ["./src/index"]
  },
  "include": ["**/*.ts", "**/*.tsx", "src/components/Planner/Calendar/utils.jsx"],
  "exclude": ["node_modules", "deployManager.ts"]
}
